<!DOCTYPE>
<html>
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="/styles/material.min.css">
  <link rel="stylesheet" href="/styles/styles.css">
  <link href="https://fonts.googleapis.com/css?family=Roboto:regular,bold,italic,thin,light,bolditalic,black,medium&amp;lang=en" rel="stylesheet">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
  <body>
    <div class="mdl-layout mdl-js-layout mdl-layout--fixed-drawer
	     mdl-layout--fixed-header">
       <header class="mdl-layout__header mdl-color--white mdl-color--grey-100 mdl-color-text--grey-600 is-casting-shadow ">
      	 <div class="mdl-layout__header-row">
	  <!-- <div class="mdl-layout-spacer"></div>-->
            <div class="searchBox mdl-textfield mdl-js-textfield mdl-textfield--expandable  mdl-textfield--align-left mdl-textfield--full-width is-focused">
              <label class="mdl-button mdl-js-button mdl-button--icon" for="productSearch">
	        <i class="material-icons">search</i>
	      </label>
              <div class="mdl-textfield__expandable-holder">
                <input  type="text" id="productSearch" class="mdl-textfield__input">
                <label class="mdl-textfield__label" for="productSearch"> Find Good things</label>
              </div>
            </div>
	     <div class="mdl-layout-spacer"></div>
	   </div>
       </header>
	<div class="mdl-layout__drawer mdl-color--blue-grey-800">
	  <div class="mdl-color--blue-grey-900 mdl-color-text--blue-grey-50 mld-grid">
	   <div class="mdl-cell mdl-cell--12-col drawer-profile">
                 <img src="/imgs/meme.gif" width="100px" heigth="100px" alt="user face">
	   </div>
	  </div>
	  <nav class="mdl-drawer-nav-label mdl-navigation ">
	    <a class="mdl-navigation__link" href="">
   	       <i class="mdl-color-text--blue-grey-400 material-icons" role="presentation">home</i>
	       Home
	    </a>
	  </nav>
	</div>
	<main class="mdl-layout__content">
	    <div class="mdl-grid" id="grid-product">
	      <!-- <div class="mdl-cell mdl-cell--12-col">-->

		  <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp" >
		     <div class="mdl-card__media">
		       <img src="/imgs/dog.png" alt="">
		     </div>
		     <div class="mdl-card__title">
                         <h4 class="mld-card__title-text">buit dog</h4> 		     
		     </div>
		     <div class="mdl-card__supporting-text">
		        this bautiful dog
		    </div>
		    <div class="mdl-card__actions">
		      comprar | adicionar
		    </div>
		  </div>

	       </div>
           <!-- </div>-->
	</main>
      </div>
      <button class="add-product mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored">
        <i class="material-icons">add</i>
	</button>
  </body>
  <script type="text/javascript" src="/js/material.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
  <script type="text/javascript" src="/js/main.js"></script>
</html>
